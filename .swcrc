{
  "jsc": {
    "parser": {
      "syntax": "typescript",
      "tsx": false,
      "decorators": false,
      "dynamicImport": false
    },
    "minify": {
      "compress": {
        "unused": true
      },
      "mangle": true
    }
  },
  "env": {
    "mode": "usage",
    "coreJs": 3
  },
  "minify": true,
  "sourceMaps": true
}
